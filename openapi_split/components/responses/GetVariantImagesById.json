                    }
                  }
                ]
              },
              "variant_details": {
                "allOf": [
                  {
                    "$ref": "#/components/schemas/HateoasLink"
                  },
                  {
                    "description": "Link to variant details",
                    "example": {
                      "href": "https://api.printful.com/v2/catalog-variants/4011"
                    }
                  }
                ]
              }
            }
          }
