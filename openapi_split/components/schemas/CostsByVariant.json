      },
      "variant_name": {
        "type": "string",
        "description": "Variant name."
      },
      "product_id": {
        "type": "integer",
        "description": "Product ID. See [Catalog API](#tag/Catalog-API)."
      },
      "fulfillment": {
        "type": "string",
